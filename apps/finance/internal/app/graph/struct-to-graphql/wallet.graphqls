type Wallet @shareable {
  balance: Float!
  createdAt: Date!
  creationTime: Date!
  currency: String!
  id: ID!
  markedForDeletion: Boolean
  recordVersion: Int!
  teamId: String!
  updatedAt: Date!
  updateTime: Date!
}

input WalletIn {
  balance: Float!
  createdAt: Date!
  currency: String!
  teamId: String!
  updatedAt: Date!
}

